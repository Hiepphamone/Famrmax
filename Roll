function inventory()
    return game:GetService("ReplicatedStorage").Remotes.GetInventory:InvokeServer()
end

function adumm()
    for i,v in pairs(inventory().Items) do
        if i == "Trait Crystal" then
            return v
        end
    end
    return 0
end

function adumm2()
    for i,v in pairs(inventory().Items) do
        if i == "Divine Trait Crystal" then
            return v
        end
    end
    return 0
end

function adumm3()
    for i,v in pairs(inventory().Items) do
        if i == "Spirit Orb" then
            return v
        end
    end
    return 0
end

function adumm4()
    for i,v in pairs(inventory().Items) do
        if i == "Christmas Cookie" then
            return v
        end
    end
    return 0
end

function tablefind(a,b)
    if table.find(a,b) then
        return true
    end
    return false
end

function findtrait(trait)
    if trait.Type ~= nil then
        for _, v in pairs(trait.Traits) do
            for _, v1 in pairs(getgenv().config.settrait[trait.Type]) do
                if string.find(v, v1) then
                    return true
                end
            end
        end
    else
        for _, v in pairs(trait) do
            for _, v1 in pairs(getgenv().config.settrait) do
                if tablefind(v1, v) then
                    return true
                end
            end
        end
    end
    return false
end

function getAllUnitEquiped()
    local inven = inventory()
    for i,v in pairs(inven.EquippedUnits) do
        if not findtrait(inven.Units[v]) then
            return v
        end
    end
    return true
end

function getAllUnitEquiped2()
    local inven = inventory()
    for i,v in pairs(inven.EquippedUnits) do
        if not tablefind(getgenv().config.spirit[inven.Units[v].Type], inven.Units[v].Spirit) then
            return v
        end
    end
    return true
end


function getstatus()
    local have = false
    local have2 = false
    for i,v in pairs(inventory().Units) do
        if v.Type == "Garcia" then
            have2 = true
        end
        if v.Type == "Agent Garcia" or v.Type == "Garcia" then
            have = true
        end
    end
    if not have then
        return {0, "Thieu Pet"}
    end
    if adumm4() < 5 and have2 == true then
        return {0, "Thieu Christmas"}
    end
    for i,v in pairs(inventory().Units) do
        if v.Type == "Garcia" then
            return {1, i}
        end
    end
    for i,v in pairs(inventory().Units) do
        if v.Type == "Agent Garcia" then
            if findtrait(v) then
                return {0,"HOAN THANH, TIEP TUC VOI CAC CON KHAC"}
            end
        end
    end
    for i,v in pairs(inventory().Units) do
        if v.Type == "Agent Garcia" then
            if not findtrait(v) then
                return {2,i}
            end
        end
    end
    return {0, "Hoan thanh"}
end

function webhookne(webhook, des)
    local url = webhook
    local data = {
    ["content"] = "@everyone",
        ["embeds"] = {
            {
                ["title"] = "Congratulation",
                ["description"] = "Name: " .. plr.Name .. " \nStatus: " .. des,
                ["type"] = "rich",
                ["color"] = tonumber(0xff8200),
            }
        }
    }
    local newdata = game:GetService("HttpService"):JSONEncode(data)
        
    local headers = {
        ["content-type"] = "application/json"
    }
    request = http_request or request or HttpPost or syn.request
    local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
    request(abcdef)

end

local stepbuild = {
    [0] = function(des)
        local plr = game.Players.LocalPlayer
        local webhook = getgenv().config.webhook
        if webhook then
            webhookne(webhook, des)
        end
    end,
    [1] = function(idpet)
        if getgenv().config.evolve == true then
            p63.UNIT_EVOLVE:Invoke(idpet)
        end
    end,
    [2] = function(idpet) 
        if getgenv().config.traitroll == true then
            if idpet == true then
                return
            end
            black = adumm2() > 0
            local status, info
            status, info = p63.UNIT_ROLL_TRAIT:Invoke(idpet, black)
            if black == false and adumm() <= 0 then
                getgenv().start2 = false
                webhookne(getgenv().config.webhook , "Het RR , Black RR")
                return false
            end
            if status == true then
                if findtrait(info) then
                    return true
                end
            end
            return false
        end
    end,
    [3] = function(idpet, lockspirit)
        if idpet == true then
            return
        end
        if getgenv().config.spiritroll == true then
            if adumm3() == 0 then
                webhookne(getgenv().config.webhook , "Het SPIRIT")
            else
                local status = p63.SPIRIT_SUMMON:Invoke(idpet)
                if status == true then
                    wait(1)
                    if tablefind(inventory().Units[idpet].Spirit, lockspirit) then
                        return true
                    end
                end
            end
        end
    end,
    [4] = function()
        print("DONE")
    end
}

getgenv().start = true
getgenv().start2 = false
getgenv().start3 = false
spawn(function()
    while getgenv().start do wait()
        local status = getstatus()
        if status[1] == 0 then
            -- equip unit

            local inven = inventory()
            local skipunit = {}
            for i,v in pairs(inven.Units) do
                if tablefind(getgenv().config.equipUnit, v.Type) then
                    if not tablefind(skipunit, v.Type) then
                        letfkinggo(p63.UNIT_EQUIP_UNEQUIP, i)
                        wait(3)
                        table.insert(skipunit, v.Type)
                    end
                end
            end
            getgenv().start = false
            getgenv().start2 = true
            getgenv().start3 = true
        end
        stepbuild[status[1]](status[2])
    end
end)

spawn(function()
    while true do wait(1)
        pcall(function()
            if getgenv().start2 == false then return end
            stepbuild[2](getAllUnitEquiped())
        end)
    end
end)

spawn(function()
    while true do wait(1)
        local a,b = pcall(function()
            if getgenv().start3 == false then return end
            local inven = inventory()
            print("Inhere")
            stepbuild[3](getAllUnitEquiped2(),getgenv().config.spirit[inven.Units[getAllUnitEquiped2()].Type])
        end)
        print(a,b)
    end
end)
